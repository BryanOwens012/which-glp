{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/frontend && yarn build",
  "outputDirectory": "apps/frontend/.next",
  "devCommand": "cd apps/frontend && yarn dev",
  "installCommand": "cd apps/frontend && yarn install",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  },
  "ignoreCommand": "bash -c 'if git diff --quiet HEAD^ HEAD -- apps/frontend apps/shared apps/api/src/routers .env vercel.json; then exit 1; else exit 0; fi'"
}
